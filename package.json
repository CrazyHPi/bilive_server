{
  "name": "bilive_server",
  "version": "1.0.0",
  "description": "bilibili直播监控转发程序",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:tsc && npm-run-posix-or-windows build:copy",
    "build:tsc": "tsc -p tsconfig.json || exit 0",
    "build:copy": "cp bilive/options.default.json build/bilive/",
    "build:copy:windows": "copy bilive\\options.default.json build\\bilive\\ /Y",
    "clean": "rimraf build",
    "start": "node build/app.js"
  },
  "author": "lzghzr",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.12.2",
    "@types/ws": "^6.0.1",
    "npm-run-posix-or-windows": "^2.0.2",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "ws": "^6.1.0"
  }
}
